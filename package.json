{
  "name": "basenaturaliste-backend",
  "version": "1.0.0",
  "description": "Backend for Base Naturaliste",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:debug": "tsc --sourceMap true",
    "copy:model": "copyfiles -u 1 ../basenaturaliste-model/*.ts src",
    "debug": "yarn build --sourceMap true && yarn start:debug",
    "dev": "yarn build && yarn start",
    "start": "node dist/backend",
    "start:debug": "node --inspect dist/backend"
  },
  "dependencies": {
    "lodash": "*",
    "mariadb": "*",
    "moment": "*",
    "multiparty": "*",
    "papaparse": "*"
  },
  "devDependencies": {
    "@types/lodash": "*",
    "@types/multiparty": "*",
    "@types/mysql": "*",
    "@types/node": "10",
    "copyfiles": "*",
    "tslint": "*",
    "typescript": "*",
    "typescript-tslint-plugin": "*"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
