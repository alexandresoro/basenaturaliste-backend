    "debug": "yarn build --sourceMap true && yarn start:debug",
    "dev": "yarn build && yarn start",
    "lint": "yarn eslint './src/**'",
    "start": "node dist/backend",
    "start:debug": "node --inspect dist/backend"
  },
  "dependencies": {
    "date-fns": "*",
    "date-fns-tz": "*",
    "lodash": "*",
    "mariadb": "*",
    "multiparty": "*",
    "ouca-common": "*",
    "papaparse": "*",
    "xlsx": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "@types/lodash": "*",
    "@types/multiparty": "*",
    "@types/node": "12",
    "@types/yargs": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "typescript": "*"
  },
  "engines": {
    "node": ">=12.9"
  }
}
